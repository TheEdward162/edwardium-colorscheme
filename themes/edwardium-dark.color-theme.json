{
	"name": "Edwardium",
	"type": "dark",
	"colors": {
		"tab.activeModifiedBorder": "#E6003A",
		"editor.background": "#111111",
		"editorGutter.background": "#151515",
		"editor.foreground": "#B9B5B8",
		"peekViewEditor.background": "#000000",
		"peekView.border": "#E6003A",
		"editor.lineHighlightBackground": "#FFFFFF20",
		"editor.lineHighlightBorder": "#00000000",
		"sideBar.background": "#111111",
		"sideBar.border": "#000000",
		"sideBarSectionHeader.background": "#222222",
		"activityBar.background": "#000000",
		"statusBar.background": "#000000",
		"statusBar.debuggingBackground": "#E6003A",
		"statusBar.noFolderBackground": "#003AE6",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBrightBlack": "#333333",
		"terminal.ansiRed": "#E6003A",
		"terminal.ansiBrightRed": "#E6003A",
		"terminal.ansiGreen": "#3AE600",
		"terminal.ansiBrightGreen": "#3AE600",
		"terminal.ansiYellow": "#E6AC00",
		"terminal.ansiBrightYellow": "#E6AC00",
		"terminal.ansiBlue": "#003AE6",
		"terminal.ansiBrightBlue": "#003AE6",
		"terminal.ansiMagenta": "#AC00E6",
		"terminal.ansiBrightMagenta": "#AC00E6",
		"terminal.ansiCyan": "#00E6AC",
		"terminal.ansiBrightCyan": "#00E6AC",
		"terminal.ansiWhite": "#FFFFFF",
		"terminal.ansiBrightWhite": "#AAAAAA"
	},
	"tokenColors": [
		{
			"name": "Default",
			"scope": [
				"meta.attribute.python",
				"meta.template.expression",
				"variable.other.normal.shell",
				"variable.other.bracket.shell",
				"variable.other.positional.shell",
				"variable.other.special.shell",
			],
			"settings": {
				"foreground": "#B9B5B8"
			}
		},
		{
			"name": "Grey",
			"scope": [
				"comment",
				"entity.name.label",
				"entity.name.namespace.rust",
				"variable.parameter.function-call.python",
				"entity.name.scope-resolution.cpp",
				"entity.name.scope-resolution.function.call.cpp"
			],
			"settings": {
				"foreground": "#868C86"
			}
		},
		{
			"name": "Red",
			"scope": [
				"keyword.operator",
				"keyword.other",
				"punctuation.definition.heading",
				"punctuation.definition.list",
				"punctuation.definition.block.sequence.item",
				"punctuation.definition.markdown",
				"punctuation.definition.raw.markdown",
				"punctuation.accessor.optional.ts",
				"punctuation.separator.pointer-access.cpp",
				"punctuation.separator.dot-access.cpp",
				"punctuation.separator.scope-resolution.cpp",
				"punctuation.separator.scope-resolution.function.call.cpp"
			],
			"settings": {
				"foreground": "#E6003A"
			}
		},
		{
			"name": "Red bold",
			"scope": [
				"storage",
				"keyword.control",
			],
			"settings": {
				"foreground": "#E6003A",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Green",
			"scope": [
				"string",
				"markup.inline.raw.string",
				"markup.fenced_code.block"
			],
			"settings": {
				"foreground": "#3AE600"
			}
		},
		{
			"name": "Yellow",
			"scope": [
				"constant",
				"entity.name.type",
				"entity.name.tag",
				"entity.other.inherited-class",
				"support.class.builtin",
				"support.type.primitive",
				"support.type.builtin",
				"support.type.property-name",
				"meta.link.inline",
				"support.type.python"
			],
			"settings": {
				"foreground": "#E6AC00"
			}
		},
		{
			"name": "Yellow bold",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "#E6AC00",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Orange",
			"scope": [
				"variable.language.super",
				"variable.language.self"
			],
			"settings": {
				"foreground": "#E63900"
			}
		},
		{
			"name": "Blue",
			"scope": [
				"entity.name.function.macro",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.definition.variable.shell"
			],
			"settings": {
				"foreground": "#003AE6"
			}
		},
		{
			"name": "Magenta",
			"scope": [
				"entity.name.type.lifetime"
			],
			"settings": {
				"foreground": "#AC00E6"
			}
		},
		{
			"name": "Cyan",
			"scope": [
				"entity.name.function",
				"meta.attribute",
				"entity.name.section",
				"meta.function-call.generic.python",
				"support.function.builtin.python"
			],
			"settings": {
				"foreground": "#00E6AC"
			}
		},
		{
			"name": "White",
			"scope": [
				"punctuation.definition.attribute",
				"variable.other.constant.property",
				"constant.other.caps",
				"variable.other.enummember",
				"variable.other.constant",
				"punctuation.section.array.shell"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		}
	]
}